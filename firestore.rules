rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
    match /{projects} {
      allow read, write, update, delete: if request.auth!=null;
        }
        match /{taskLibrary} {
          allow read, write, update, delete: if request.auth!=null;
        }
        match /{users} {
          allow read, write, update, delete: if request.auth!=null;
        }
        match /{contactMessage} { allow read, write }
    }
  }
}